{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "InMemoryDatabaseName": "UserManagementDb"
  },
  "Cors": {
    "PolicyName": "AllowAngularApp",
    "AngularAppOrigin": "http://localhost:4200"
  },
  "ExternalApi": {
    "JsonPlaceholderUrl": "https://jsonplaceholder.typicode.com/users",
    "GravatarBaseUrl": "https://www.gravatar.com/avatar"
  }
}
